<%= turbo_frame_tag "post_form_#{post.id}" do %>
<p> for edit </p>
  <%= form_with(model: post, local: false) do |form| %>
    <%= render "error", post: post %>

    <div class="field">
      <%= form.label :title %>
      <%= form.text_field :title %>
    </div>

    <div class="field">
      <%= form.label :body %>
      <%= form.text_area :body %>
    </div>

    <div class="actions">
      <%= form.submit %>
    </div>
  <% end %>
<% end %>
